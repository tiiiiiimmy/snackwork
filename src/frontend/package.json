{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:accessibility": "node scripts/accessibility-audit.js",
    "lighthouse": "lighthouse http://localhost:5173 --output-path=./reports/lighthouse.html --save-assets --view",
    "health-check": "curl -f http://localhost:5173 || exit 1",
    "analyze": "npm run test:coverage && npm run test:accessibility && npm run lighthouse",
    "ci": "npm run lint && npm run test && npm run build"
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.16.10",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.1.1",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.10.0",
    "lucide-react": "^0.525.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.65.0",
    "react-router-dom": "^7.6.3",
    "zod": "^3.25.75"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/google.maps": "^3.58.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "cypress": "^13.16.0",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "jsdom": "^26.0.0",
    "lighthouse": "^12.8.0",
    "msw": "^2.8.1",
    "sass": "^1.89.2",
    "sass-embedded": "^1.89.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^5.4.2",
    "vitest": "^2.1.8"
  }
}